  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adex001/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/adex001/Book-A-Meal#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "request": "^2.85.0",
    "sequelize": "^4.37.6",
    "should": "^13.2.1",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
